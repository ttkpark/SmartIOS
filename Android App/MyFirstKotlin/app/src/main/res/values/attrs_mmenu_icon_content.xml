<resources>
    <declare-styleable name="MmenuIconContent">
        <attr name="exampleString" format="string" />
        <attr name="exampleDimension" format="dimension" />
        <attr name="exampleColor" format="color" />
        <attr name="exampleDrawable" format="color|reference" />

        <attr name="ContentOrientation" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
            <enum name="circle" value="2"/>
            <enum name="downwards" value="3"/>
        </attr>
        <attr name="BackColor" format="color" />
        <attr name="IconSize" format="dimension" />
        <attr name="TextSize" format="dimension" />
        <attr name="MarginVertical" format="dimension" />
        <attr name="MarginLeft" format="dimension" />
        <attr name="MarginRight" format="dimension" />
        <attr name="iconSrc" format="reference" />
        <attr name="Title" format="string" />
        <attr name="textColor" format="color" />
        <attr name="subTitleColor" format="color" />
        <attr name="SubTitleSize" format="dimension" />
        <attr name="subTitle" format="string" />
        <attr name="subTitleVisible" format="boolean" value="true" />

    </declare-styleable>

</resources>